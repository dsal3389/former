{
  "name": "former",
  "version": "0.1.0",
  "background": {
    "scripts": ["background/background.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "scripting",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "src/popup/index.html",
    "default_title": "former - do not dwell on the past",
    "default_icon": {
      "32": "former.png"
    }
  },
  "manifest_version": 2
}
