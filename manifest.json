{
  "name": "former",
  "version": "0.1.0",
  "background": {
    "scripts": ["background/background.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "nativeMessaging",
    "<all_urls>"
